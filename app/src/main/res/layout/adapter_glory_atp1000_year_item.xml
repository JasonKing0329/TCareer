<?xml version="1.0" encoding="utf-8"?>
<layout>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent" android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tv_year"
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:paddingTop="12dp"
                android:paddingBottom="12dp"
                android:layout_margin="1px"
                android:gravity="center"/>
            <TextView
                android:id="@+id/tv_iw"
                style="@style/textGloryAtp1000"
                android:background="@drawable/selector_table_gs_item_hard" />
            <TextView
                android:id="@+id/tv_mm"
                style="@style/textGloryAtp1000"
                android:background="@drawable/selector_table_gs_item_hard"/>
            <TextView
                android:id="@+id/tv_mc"
                style="@style/textGloryAtp1000"
                android:background="@drawable/selector_table_gs_item_clay"/>
            <TextView
                android:id="@+id/tv_md"
                style="@style/textGloryAtp1000"
                android:background="@drawable/selector_table_gs_item_clay"/>
            <TextView
                android:id="@+id/tv_ro"
                style="@style/textGloryAtp1000"
                android:background="@drawable/selector_table_gs_item_clay"/>
            <TextView
                android:id="@+id/tv_rc"
                style="@style/textGloryAtp1000"
                android:background="@drawable/selector_table_gs_item_hard"/>
            <TextView
                android:id="@+id/tv_cc"
                style="@style/textGloryAtp1000"
                android:background="@drawable/selector_table_gs_item_hard"/>
            <TextView
                android:id="@+id/tv_sh"
                style="@style/textGloryAtp1000"
                android:background="@drawable/selector_table_gs_item_hard"/>
            <TextView
                android:id="@+id/tv_pa"
                style="@style/textGloryAtp1000"
                android:background="@drawable/selector_table_gs_item_hard"/>
        </LinearLayout>

    </RelativeLayout>
</layout>